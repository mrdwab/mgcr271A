get_info <- function(data, pkg = 'datasets', type = 'description'){
require(tools)
db = Rd_db(pkg)
dbi = db[grep(data, names(db))]
info = lapply(dbi, tools:::.Rd_get_metadata, type)
info = paste(info[[1]], collapse = " ")
setNames(info, type)
}
